<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:simpleType name="T_CadUnits">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="MILLIMETER"/>
      <xsd:enumeration value="MICRON"/>
      <xsd:enumeration value="INCH"/>
      <xsd:enumeration value="MIL"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_ClassText">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="-"/>
      <xsd:enumeration value="NA"/>
      <xsd:enumeration value="false"/>
      <xsd:enumeration value="true"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_Context">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BOARD"/>
      <xsd:enumeration value="BOARDPANEL"/>
      <xsd:enumeration value="ASSEMBLY"/>
      <xsd:enumeration value="ASSEMBLYARRAY"/>
      <xsd:enumeration value="DOCUMENTATION"/>
      <xsd:enumeration value="TOOLING"/>
      <xsd:enumeration value="COUPON"/>
      <xsd:enumeration value="MISC"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_CouponPosition">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="t"/>
      <xsd:enumeration value="b"/>
      <xsd:enumeration value="l"/>
      <xsd:enumeration value="r"/>
      <xsd:enumeration value="ch"/>
      <xsd:enumeration value="cv"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_DecString">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="-?\d+(\.\d*)?|NA|NE|NR|disabled|unknown|-"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_Drill">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CARBIDE"/>
      <xsd:enumeration value="ROUTER"/>
      <xsd:enumeration value="LASER"/>
      <xsd:enumeration value="FLATNOSE"/>
      <xsd:enumeration value="EXTENSION"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_DrillSequenceType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="backdrill"/>
      <xsd:enumeration value="buried"/>
      <xsd:enumeration value="blind"/>
      <xsd:enumeration value="NPTH"/>
      <xsd:enumeration value="PTH"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_Email">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[^@]+@[^\.]+\..+|NA|unknown"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_IntString">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="-?\d+|NA|NR|unknown"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_LayerFunction">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BOARDOUTLINE"/>
      <xsd:enumeration value="COATINGCOND"/>
      <xsd:enumeration value="COATINGNONCOND"/>
      <xsd:enumeration value="DIELBASE"/>
      <xsd:enumeration value="DIELCORE"/>
      <xsd:enumeration value="DIELPREG"/>
      <xsd:enumeration value="DIELADHV"/>
      <xsd:enumeration value="COMPONENT"/>
      <xsd:enumeration value="COURTYARD"/>
      <xsd:enumeration value="SOLDERBUMP"/>
      <xsd:enumeration value="PASTEMASK"/>
      <xsd:enumeration value="HOLEFILL"/>
      <xsd:enumeration value="SIGNAL"/>
      <xsd:enumeration value="POWER_GROUND"/>
      <xsd:enumeration value="MIXED"/>
      <xsd:enumeration value="RESISTIVE"/>
      <xsd:enumeration value="CAPACITIVE"/>
      <xsd:enumeration value="SILKSCREEN"/>
      <xsd:enumeration value="SOLDERPASTE"/>
      <xsd:enumeration value="SOLDERMASK"/>
      <xsd:enumeration value="DRILL"/>
      <xsd:enumeration value="GLUE"/>
      <xsd:enumeration value="ROUT"/>
      <xsd:enumeration value="LEGEND"/>
      <xsd:enumeration value="GRAPHIC"/>
      <xsd:enumeration value="PROBE"/>
      <xsd:enumeration value="PIN"/>
      <xsd:enumeration value="REWORK"/>
      <xsd:enumeration value="DOCUMENT"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_NonNegativeDouble">
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="0.0"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_Polarity">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="POSITIVE"/>
      <xsd:enumeration value="NEGATIVE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_Present">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="yes"/>
      <xsd:enumeration value="no"/>
      <xsd:enumeration value="unknown"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_Side">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TOP"/>
      <xsd:enumeration value="BOTTOM"/>
      <xsd:enumeration value="INNER"/>
      <xsd:enumeration value="BOTH"/>
      <xsd:enumeration value="NONE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_UnitOfArea">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="mm"/>
      <xsd:enumeration value="mil"/>
      <xsd:enumeration value="inch"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_UnitOfDistance">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value=""/>
      <xsd:enumeration value="mm"/>
      <xsd:enumeration value="mil"/>
      <xsd:enumeration value="inch"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_UnitMode">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="DISTANCE"/>
      <xsd:enumeration value="AREA"/>
      <xsd:enumeration value="RESISTANCE"/>
      <xsd:enumeration value="CAPACITANCE"/>
      <xsd:enumeration value="IMPEDANCE"/>
      <xsd:enumeration value="PERCENTAGE"/>
      <xsd:enumeration value="SIZE"/>
      <xsd:enumeration value="NONE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="T_WhereMeasured">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="LAMINATE"/>
      <xsd:enumeration value="METAL"/>
      <xsd:enumeration value="MASK"/>
      <xsd:enumeration value="OTHER"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ST_ToolFunction">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="via"/>
      <xsd:enumeration value="laservia"/>
      <xsd:enumeration value="component"/>
      <xsd:enumeration value="mechanical"/>
      <xsd:enumeration value="castellated"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="T_Array">
    <xsd:attribute name="nrInX" type="xsd:string" use="required"/>
    <xsd:attribute name="nrInY" type="xsd:string" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_BareBoardTestCharacteristics">
    <xsd:sequence>
      <xsd:element name="Units" type="T_Units" minOccurs="0"/>
      <xsd:element name="BareBoardTestLayer" type="T_BareBoardTestLayer" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="nbNets" type="xsd:integer" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_BareBoardTestLayer">
    <xsd:attribute name="layerOrGroupRef" type="xsd:string" use="required"/>
    <xsd:attribute name="testablePads" type="xsd:string"/>
    <xsd:attribute name="maxTPDensity" type="xsd:string"/>
    <xsd:attribute name="nrOfSMDs" type="xsd:string"/>
    <xsd:attribute name="smallestSMDPitch" type="T_DecString"/>
    <xsd:attribute name="smallestSMDSize" type="T_DecString"/>
    <xsd:attribute name="nrOfSmallestSMDSize" type="T_IntString"/>
    <xsd:attribute name="nrEdgeConnectorFingers" type="xsd:integer" use="optional"/>
    <xsd:attribute name="edgeConnectorWidth" type="T_DecString" use="optional"/>
    <xsd:attribute name="edgeConnectorHeight" type="T_DecString" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="T_BoardCharacteristics">
    <xsd:sequence>
      <xsd:element name="Units" type="T_Units" minOccurs="0"/>
      <xsd:element name="Dimension" type="T_Dimension" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="T_Border">
    <xsd:attribute name="bottom" type="xsd:decimal"/>
    <xsd:attribute name="left" type="xsd:decimal"/>
    <xsd:attribute name="right" type="xsd:decimal"/>
    <xsd:attribute name="top" type="xsd:decimal"/>
  </xsd:complexType>
  <xsd:complexType name="T_CadData">
    <xsd:annotation>
      <xsd:documentation>IPC2581 CadDataType without StepList element</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="LayerDesc" type="T_LayerDesc"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="T_CadHeader">
    <xsd:annotation>
      <xsd:documentation>IPC2581 CadHeaderType without Spec, SurfaceFinish,ChangeRec elements</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="units" type="T_CadUnits" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_Carbon">
    <xsd:sequence>
      <xsd:element name="CarbonCharacteristics" type="T_CarbonCharacteristics" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="presence" type="xsd:string" use="required"/>
    <xsd:attribute name="nblayers" type="xsd:integer" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_CarbonCharacteristics">
    <xsd:sequence>
      <xsd:element name="CarbonLayer" type="T_CarbonLayer" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="T_CarbonLayer">
    <xsd:sequence>
      <xsd:element name="MinTrack" type="T_MinResult" minOccurs="0"/>
      <xsd:element name="MinGap" type="T_MinResult" minOccurs="0"/>
      <xsd:element name="MinDistToPlated" type="T_MinResult" minOccurs="0"/>
      <xsd:element name="MinDistToOutline" type="T_MinResult" minOccurs="0"/>
      <xsd:element name="LayerArea" type="T_LayerArea" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="layerOrGroupRef" type="xsd:string"/>
    <xsd:attribute name="side" type="xsd:string"/>
  </xsd:complexType>
  <xsd:complexType name="T_Class">
    <xsd:simpleContent>
      <xsd:extension base="T_ClassText">
        <xsd:attribute name="id" use="required"/>
        <xsd:attribute name="label" use="required"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="T_Clearance">
    <xsd:attribute name="x" type="xsd:decimal" use="required"/>
    <xsd:attribute name="y" type="xsd:decimal" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_Contact">
    <xsd:sequence>
      <xsd:element name="Email" type="T_Email"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="T_CopperArea">
    <xsd:simpleContent>
      <xsd:extension base="T_DecString">
        <xsd:attribute name="unitMode" type="T_UnitMode" use="required"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="T_CopperCharacteristics">
    <xsd:sequence>
      <xsd:element name="Units" type="T_Units" minOccurs="0"/>
      <xsd:element name="CopperLayer" type="T_CopperLayer" maxOccurs="unbounded"/>
      <xsd:element name="CopperRingByLayerAndTool" type="T_CopperRingByLayerAndTool" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="CopperRingByLayerAndToolFunction" type="T_CopperRingByLayerAndToolFunction" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="CopperToDrillClearance" type="T_CopperToDrillClearance" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_CopperLayer">
    <xsd:sequence>
      <xsd:element name="MinTrack" type="T_MinResult"/>
      <xsd:element name="MinTrackAllCopper" type="T_MinResult"/>
      <xsd:element name="MinTrackCriticalTrace" type="T_MinResult"/>
      <xsd:element name="MinTrackAllTrace" type="T_MinResult"/>
      <xsd:element name="MinGap" type="T_MinResult"/>
      <xsd:element name="MinSelfSpacing" type="T_MinResult"/>
      <xsd:element name="MinGapPP" type="T_MinResult"/>
      <xsd:element name="MinGapPT" type="T_MinResult"/>
      <xsd:element name="MinGapTT" type="T_MinResult"/>
      <xsd:element name="MinGapTraceTrace" type="T_MinResult" minOccurs="0"/>
      <xsd:element name="MinRing" type="T_MinResult"/>
      <xsd:element name="MinDistCopperToPlated" type="T_MinResult"/>
      <xsd:element name="MinDistCopperToNonPlated" type="T_MinResult"/>
      <xsd:element name="MinDistCopperToBackdrill" type="T_MinResult"/>
      <xsd:element name="MinDistCopperToOutline" type="T_MinResult"/>
      <xsd:element name="MinDistCopperToOutlinePads" type="T_MinResult" minOccurs="0"/>
      <xsd:element name="MinDistCopperToOutlineTracks" type="T_MinResult" minOccurs="0"/>
      <xsd:element name="MinDistCopperToOutlineRegions" type="T_MinResult" minOccurs="0"/>
      <xsd:element name="MinDistCopperToScoredOutline" type="T_MinResult" maxOccurs="4"/>
      <xsd:element name="MinDistViaPlug" type="T_MinResult"/>
      <xsd:element name="CopperArea" type="T_CopperArea"/>
      <xsd:element name="EtchCompensation" type="T_DecString" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="layerOrGroupRef" type="xsd:string" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_CopperRingByLayerAndTool">
    <xsd:sequence>
      <xsd:element name="MinRing" type="T_MinRingByLayerAndTool" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="layerOrGroupRef" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_CopperRingByLayerAndToolFunction">
    <xsd:sequence>
      <xsd:element name="MinRing" type="T_MinRingByLayerAndToolFunction" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="layerOrGroupRef" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_CopperToDrillClearance">
    <xsd:sequence>
      <xsd:element name="MinClrByLayerAndTool" type="T_MinClrByLayerAndTool" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="id_Type" use="required">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="PadToPlated"/>
          <xsd:enumeration value="TrackToPlated"/>
          <xsd:enumeration value="PadToNonPlated"/>
          <xsd:enumeration value="TrackToNonPlated"/>
          <xsd:enumeration value="PadToBackdrill"/>
          <xsd:enumeration value="TrackToBackdrill"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="T_Coupon">
    <xsd:sequence>
      <xsd:element name="Size" type="T_Size"/>
      <xsd:element name="Array" type="T_Array"/>
      <xsd:element name="CouponCouponClearance" type="T_Clearance"/>
      <xsd:element name="CouponPcbClearance" type="T_Clearance"/>
      <xsd:element name="Position" type="T_CouponPosition"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="T_Customer">
    <xsd:sequence>
      <xsd:element name="Name" type="xsd:string"/>
      <xsd:element name="Contact" type="T_Contact" maxOccurs="unbounded"/>
      <xsd:element name="BoardRef" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="T_CustomerPanel">
    <xsd:sequence>
      <xsd:element name="CustomerPanelIn" type="T_CustomerPanelPresent"/>
      <xsd:element name="CustomerPanelOut" type="T_CustomerPanelPresent"/>
      <xsd:element name="Units" type="T_Units" minOccurs="0"/>
      <xsd:element name="PanelSize" type="T_Size" minOccurs="0"/>
      <xsd:element name="Offset" type="T_Offset" minOccurs="0"/>
      <xsd:element name="Spacing" type="T_Size" minOccurs="0"/>
      <xsd:element name="Array" type="T_Array" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="ref" type="xsd:string"/>
    <xsd:attribute name="present" type="xsd:boolean"/>
  </xsd:complexType>
  <xsd:complexType name="T_CustomerPanelPresent">
    <xsd:attribute name="present" type="T_Present" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_Dfm">
    <xsd:sequence>
      <xsd:element name="Rule" type="T_DfmRule" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required"/>
    <xsd:attribute name="class" type="xsd:integer" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_DfmRule">
    <xsd:sequence>
      <xsd:element name="Class" type="T_Class" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required"/>
    <xsd:attribute name="group" type="xsd:string" use="required"/>
    <xsd:attribute name="name" type="xsd:string" use="required"/>
    <xsd:attribute name="value" type="xsd:string" use="required"/>
    <xsd:attribute name="applicable" type="xsd:boolean" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_Dimension">
    <xsd:sequence>
      <xsd:element name="Width" type="T_DecString"/>
      <xsd:element name="Height" type="T_DecString"/>
      <xsd:element name="Type" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Perimeter" type="T_DecString"/>
      <xsd:element name="Area" type="T_LayerArea"/>
    </xsd:sequence>
    <xsd:attribute name="source" type="xsd:string" use="required"/>
    <xsd:attribute name="layerOrGroupRef" type="xsd:string" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="T_DrillCharacteristics">
    <xsd:sequence>
      <xsd:element name="Units" type="T_Units" minOccurs="0"/>
      <xsd:element name="DrillGroup" type="T_DrillGroup" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_DrillGroup">
    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
      <xsd:element name="DrillUse" type="T_DrillUse"/>
    </xsd:sequence>
    <xsd:attribute name="layerOrGroupRef" type="xsd:string" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_DrillSequence">
    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
      <xsd:element name="Span" type="T_Span" minOccurs="0"/>
      <xsd:element name="Tools" type="xsd:integer"/>
      <xsd:element name="MinEndDia" type="T_DecString"/>
      <xsd:element name="MaxEndDia" type="T_DecString"/>
      <xsd:element name="MinToolDia" type="T_DecString" minOccurs="0"/>
      <xsd:element name="MaxToolDia" type="T_DecString" minOccurs="0"/>
      <xsd:element name="Holes" type="xsd:integer"/>
      <xsd:element name="MinRingOnOuter" type="T_MinResult"/>
      <xsd:element name="MinRingOnInner" type="T_MinResult"/>
      <xsd:element name="MinClrCopper" type="T_MinResult"/>
      <xsd:element name="MinClr" type="T_MinResult" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Optional by license</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OverlappingHoles" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Optional by license</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MinClrInnerSpan" type="T_MinResult" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Optional by license</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MinClrOutline" type="T_MinResult"/>
      <xsd:element name="MinClrOutlineTracks" type="T_MinResult" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="type" type="T_DrillSequenceType" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_DrillSequences">
    <xsd:sequence>
      <xsd:element name="DrillSequence" type="T_DrillSequence" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_DrillTool">
    <xsd:attribute name="number" type="xsd:double" use="required"/>
    <xsd:attribute name="name" type="xsd:string" use="required"/>
    <xsd:attribute name="type" type="T_Drill" use="optional"/>
    <xsd:attribute name="tolPlus" type="T_DecString" use="required"/>
    <xsd:attribute name="tolMinus" type="T_DecString" use="required"/>
    <xsd:attribute name="finishSize" type="T_DecString" use="required"/>
    <xsd:attribute name="drillSize" type="T_DecString" use="required"/>
  </xsd:complexType>
  <xsd:complexType name="T_DrillUse">
    <xsd:attribute name="drillToolRef" use="required"/>
    <xsd:attribute name="type" use="required"/>
    <xsd:attribute name="count" use="required"/>
    <xsd:attribute name="moves" use="required"/>
    <xsd:attribute name="method" use="required"/>
    <xsd:attribute name="filledVia" use="required"/>
    <xsd:attribute name="countered" use="required"/>
    <xsd:attribute name="doubles" use="required"/>
    <xsd:attribute name="predrillHits" use="required"/>
    <xsd:attribute name="minRing" use="required"/>
    <xsd:attribute name="minRingOnInner" use="required"/>
    <xsd:attribute name="minRingOnOuter" use="required"/>
    <xsd:attribute name="minPadSize" use="required"/>
    <xsd:attribute name="platedToolWithMissingCopperRings" use="optional"/>
    <xsd:attribute name="NPTHToolTouchingFunctionalCopper" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="T_EdgeConnectors">
    <xsd:sequence>
      <xsd:element name="EdgeConnectorsByLayer" type="T_EdgeConnectorByLayer" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="T_EdgeConnectorByLayer">
    <xsd:attribute name="layerOrGroupRef" type="xsd:string" use="required"/>
    <xsd:attribute name="nrEdgeConnectorFingers" type="xsd:integer" use="optional"/>
    <xsd:attribute name="edgeConnectorWidth" type="T_DecString" use="optional"/>
    <xsd:attribute name="edgeConnectorHeight" type="T_DecString" use="optional"/>
    <xsd:attribute name="maskCalculated" use="optional"/>
    <xsd:attribute name="side" use="optional"/>
    <xsd:attribute name="area" use="optional"/>
  </xsd:complexType>
  <xsd:complexType name="T_ExposedCopper">
    <xsd:sequence>
      <xsd:element name="Units" type="T_Units" minOccurs="0"/>
      <xsd:element name="Area" maxOccurs="5">
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="T_DecString">
              <xsd:attribute name="layerOrGroupRef"/>
              <xsd:attribute name="side"/>
              <xsd:attribute name="unitMode"/>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="boardThickness"/>
  </xsd:complexType>
  <xsd:complexType name="T_Frames">
    <xsd:sequence>
      <xsd:element name="Frame" type="T_PanelFrame" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="T_GeneratedBy">
